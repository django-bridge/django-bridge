{
    "name": "@djream/root",
    "version": "1.0.0",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "check-format": "cd .. && prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md,yml,yaml}\"",
        "format": "cd .. && prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md,yml,yaml}\""
    },
    "dependencies": {
        "prettier": "^2.7.1"
    }
}
