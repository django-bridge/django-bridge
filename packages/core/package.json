{
  "name": "@django-bridge/core",
  "version": "0.1.0",
  "description": "The simple way to build Django applications with modern React frontends",
  "repository": "https://github.com/kaedroho/django-render",
  "license": "BSD-3-Clause",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format cjs --dts --minify --clean --out-dir dist",
    "check-types": "tsc --noEmit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "react": "^18.2.0",
    "telepath-unpack": "^0.0.3"
  },
  "devDependencies": {
    "@types/node": "^12.0.0",
    "@types/react": "18",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  }
}
